generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Mandat {
  id                Int      @id @default(autoincrement())
  reference         String
  typeOffreCode     String
  typeOffre         String
  corps             String
  prix              Int
  charges           Int?
  foncier           Int?
  typeMandat        String
  typeBien          String
  typeBienCode      String
  surfaceHabitable  Int?
  nbPieces          Int?
  chambres          Int?
  nbEtages          Int?
  etage             Int?
  sdb               Int?
  wc                Int?
  cuisine           Int?
  energieChauffage  String?
  formatChauffage   String?
  parking           Boolean?
  piscine           Boolean?
  terrasse          Boolean?
  exposition        String?
  anneeConstruction Int?
  ascenseur         Boolean?
  balcon            Int?
  ville             String
  cp                String
  departement       String
  isNotAvailable    Boolean?
  statut            String?
  meuble            Boolean?
  dateEnr           DateTime?
  dateMaj           DateTime?
  latitude          Float?
  longitude         Float?
  videoLink         String?
  urlBien           String?
  publishedInWebSite Boolean?
  publishedInApp     Boolean?
  visiteImmediat     Boolean?
  bienCategory       String?
  chauffages         String?
}